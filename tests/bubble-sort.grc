import io

def main() : int {
  var v[10]: int;
  var a, b: bool;
  var c = true, d = !c: bool;
  var i : int;
  var k = 0, j, aux: int;
  var trocou = true: bool;
  var n = 10 : int;

  for (i=0; i<10; i+=1) {
    write "A[%d] = ", i;
    read v[i];
  }

  write "-------------------";

  while (k < n-1 && trocou) {
    trocou = false;
    for (j=0; j<(n-k-1); j+=1) {
      if (v[j] > v[j+1]) {
        aux = v[j];
        v[j] = v[j+1];
        v[j+1] = aux;
        trocou = true;
      }
    }
    k += 1;
  }

  for (i=0; i<10; i+=1) {
    write "%d", v[i];
  }
  return 0;
}
